{
  "name": "typescript-calendar",
  "version": "1.0.0",
  "description": "A TypeScript Calendar component for React",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "private": false,
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "watch": "tsc -watch",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "react",
    "typescript",
    "calendar"
  ],
  "author": "Jack Pickard",
  "license": "ISC",
  "peerDependencies": {
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "^4.5.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.8",
    "jest": "^29.5.0",
    "prettier": "^2.8.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "ts-jest": "^29.0.5",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@types/react-dom": "18.0.8",
    "classnames": "^2.3.2",
    "moment": "^2.29.4",
    "react": "18.2.0",
    "sass": "^1.59.3"
  }
}
